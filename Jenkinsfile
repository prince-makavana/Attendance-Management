pipeline {
  agent any // This runs the pipeline on any available agent

  stages {
    stage('Build') { // Define a stage for this step
      steps {
        echo 'Building started....' // Print this message to the console
      }
    }
    stage('Test') {
      steps {
        echo 'Testing started....'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploying started....'
      }
    }
  }
}
